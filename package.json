{
  "name": "ucl-assistant-app",
  "version": "5.0.0",
  "private": true,
  "description": "An app to manage your UCL life",
  "license": "MIT",
  "author": "UCL API",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "node scripts/build.js",
    "format": "eslint . --fix --ext .js --ext .jsx --cache --ignore-path .eslintignore",
    "lint:staged": "lint-staged",
    "publish": "node scripts/publish.js",
    "start": "expo start",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "test": "jest --ci && codecov",
    "test:dev": "jest --watch --coverage=false",
    "upload": "node scripts/upload.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "eslint --fix",
      "git add"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "pattern": "**/*-stories.js"
    }
  },
  "jest": {
    "collectCoverage": true,
    "preset": "jest-expo",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|jest-expo/.*|@unimodules/.*|sentry-expo|native-base|react-native-action-button|redux-persist.*|@sentry/react-native)"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.6",
    "axios": "^0.19.0",
    "babel-core": "^7.0.0-bridge.0",
    "base-64": "^0.1.0",
    "expo": "^35.0.1",
    "expo-analytics-amplitude": "~7.0.0",
    "expo-asset": "~7.0.0",
    "expo-blur": "~7.0.0",
    "expo-constants": "^7.0.1",
    "expo-font": "~7.0.0",
    "expo-intent-launcher": "~7.0.0",
    "expo-linear-gradient": "~7.0.1",
    "expo-localization": "~7.0.0",
    "expo-mail-composer": "~7.0.0",
    "expo-permissions": "~7.0.0",
    "expo-secure-store": "~7.0.0",
    "expo-web-browser": "~7.0.0",
    "lottie-react-native": "~2.6.1",
    "memoize-one": "^5.1.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-moment-proptypes": "^1.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-35.0.0.tar.gz",
    "react-native-action-button": "^2.8.5",
    "react-native-gesture-handler": "~1.3.0",
    "react-native-maps": "~0.25.0",
    "react-native-modal-datetime-picker": "^7.5.0",
    "react-native-reanimated": "~1.2.0",
    "react-native-svg": "9.9.2",
    "react-native-svg-charts": "^5.3.0",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.5.6",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-persist-expo-securestore": "github:uclapi/redux-persist-expo-securestore",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sentry-expo": "~2.0.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-knobs": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/react-native": "^5.2.6",
    "@welldone-software/why-did-you-render": "^3.3.9",
    "babel-eslint": "^10.0.3",
    "babel-preset-expo": "^7.1.0",
    "codecov": "^3.6.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.4.1",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-no-secrets": "^0.3.4",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-native": "^3.8.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-simple-import-sort": "^4.0.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-sort-keys-fix": "^1.1.0",
    "expo-cli": "^3.4.1",
    "husky": "^3.0.9",
    "inquirer": "^7.0.0",
    "jest-expo": "^35.0.0",
    "lint-staged": "^9.4.2",
    "react-dom": "16.8.3",
    "react-native-storybook-loader": "^1.8.1",
    "react-test-renderer": "16.8.3",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  }
}
