{
  "name": "ucl-assistant-app",
  "version": "7.0.0",
  "private": true,
  "description": "An app to manage your UCL life",
  "homepage": "https://github.com/uclapi/ucl-assistant-app",
  "bugs": {
    "url": "https://github.com/uclapi/ucl-assistant-app/issues",
    "email": "isd.apiteam@ucl.ac.uk"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uclapi/ucl-assistant-app"
  },
  "license": "MIT",
  "author": "UCL API",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "build": "node scripts/build.js",
    "format": "eslint . --fix --ext .ts --ext tsx --ext .js --ext .jsx --cache --ignore-path .eslintignore",
    "lint:staged": "lint-staged",
    "publish": "node scripts/publish.js",
    "start": "expo start",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "test": "jest --ci && codecov",
    "test:dev": "jest --watch --coverage=false",
    "upload": "node scripts/upload.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,d.ts}": [
      "tsc --allowSyntheticDefaultImports --jsx react-native --noEmit --esModuleInterop --moduleResolution node --skipLibCheck --resolveJsonModule"
    ],
    "*.{ts,tsx,js,jsx,json}": [
      "eslint --fix --ignore-pattern"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "pattern": "**/*-stories.js"
    }
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.0",
    "@react-native-community/datetimepicker": "2.2.2",
    "@react-native-community/masked-view": "0.1.6",
    "@react-native-community/viewpager": "3.3.0",
    "@react-navigation/bottom-tabs": "^5.5.2",
    "@react-navigation/material-top-tabs": "^5.2.10",
    "@react-navigation/native": "^5.5.1",
    "@react-navigation/stack": "^5.4.2",
    "axios": "^0.19.2",
    "base-64": "^0.1.0",
    "expo": "^37.0.0",
    "expo-analytics-amplitude": "~8.1.0",
    "expo-asset": "~8.1.5",
    "expo-auth-session": "~1.2.1",
    "expo-blur": "~8.1.0",
    "expo-constants": "~9.0.0",
    "expo-device": "~2.1.0",
    "expo-font": "~8.1.0",
    "expo-intent-launcher": "~8.1.0",
    "expo-linear-gradient": "~8.1.0",
    "expo-localization": "~8.1.0",
    "expo-mail-composer": "~8.1.0",
    "expo-network": "~2.1.0",
    "expo-permissions": "~8.1.0",
    "expo-secure-store": "~8.1.0",
    "expo-store-review": "~2.1.0",
    "expo-web-browser": "~8.2.1",
    "lodash": "^4.17.15",
    "lottie-react-native": "~2.6.1",
    "memoize-one": "^5.1.1",
    "moment": "^2.26.0",
    "moment-timezone": "^0.5.28",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-moment-proptypes": "^1.7.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-action-button": "github:uclapi/react-native-action-button",
    "react-native-check-box": "^2.1.7",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "0.26.1",
    "react-native-reanimated": "~1.7.0",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-svg": "11.0.1",
    "react-native-svg-charts": "^5.3.0",
    "react-native-webview": "8.1.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^5.10.0",
    "redux-persist-expo-securestore": "github:uclapi/redux-persist-expo-securestore#v3.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "sentry-expo": "^2.1.2",
    "shortid": "^2.2.15"
